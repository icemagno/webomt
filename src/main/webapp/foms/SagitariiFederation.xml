<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<objectModel xsi:schemaLocation="http://standards.ieee.org/IEEE1516-2010 http://standards.ieee.org/downloads/1516/1516.2-2010/IEEE1516-DIF-2010.xsd" xmlns="http://standards.ieee.org/IEEE1516-2010" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelIdentification>
        <name>Sagitarii Federation FOM</name>
        <type>FOM</type>
        <version>1.0</version>
        <modificationDate>2015-11-13</modificationDate>
        <securityClassification>Unclassified</securityClassification>
        <purpose>Basic specifications to the Federation</purpose>
        <applicationDomain>Engineering</applicationDomain>
        <description>FOM for the sagitarii Federation.</description>
        <useLimitation>None</useLimitation>
        <poc>
            <pocType>Primary author</pocType>
            <pocName>Carlos Magno Abreu</pocName>
            <pocOrg>CEFET</pocOrg>
            <pocTelephone>+55 55 55 55</pocTelephone>
            <pocEmail>magno.mabreu@gmail.com</pocEmail>
        </poc>
        <reference>
            <type>Document</type>
            <identification>Utilizando HLA/RTI Como Protocolo de Comunicação para Sistemas de Processamento de Workflows Científicos (Nov 2015)</identification>
        </reference>
        <other></other>
        <glyph alt="Node" width="36" height="36" type="png"></glyph>
    </modelIdentification>
    <objects>
        <objectClass>
            <name>HLAobjectRoot</name>
			
            <objectClass>
                <name>Sagitarii</name>
                <sharing>PublishSubscribe</sharing>
                <semantics>The Sagitarii Server</semantics>
            </objectClass>
			
            <objectClass>
                <name>Teapot</name>
                <sharing>PublishSubscribe</sharing>
                <semantics>A Teapot Node</semantics>
                <attribute>
                    <name>MACAddress</name>
                    <dataType>HLAunicodeString</dataType>
                    <updateType>Static</updateType>
                    <updateCondition>NA</updateCondition>
                    <ownership>NoTransfer</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <dimensions/>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>Computer's MAC Address</semantics>
                </attribute>
                <attribute>
                    <name>SOName</name>
                    <dataType>HLAunicodeString</dataType>
                    <updateType>Static</updateType>
                    <updateCondition>NA</updateCondition>
                    <ownership>NoTransfer</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <dimensions/>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>Computer's SO Name.</semantics>
                </attribute>
                <attribute>
                    <name>MachineName</name>
                    <dataType>HLAunicodeString</dataType>
                    <updateType>Static</updateType>
                    <updateCondition>NA</updateCondition>
                    <ownership>NoTransfer</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <dimensions/>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>C.</semantics>
                </attribute>
                <attribute>
                    <name>CpuLoad</name>
                    <dataType>CpuFloat</dataType>
                    <updateType>Conditional</updateType>
                    <updateCondition>On change</updateCondition>
                    <ownership>NoTransfer</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <dimensions/>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>Current CPU load.</semantics>
                </attribute>
                <attribute>
                    <name>AvailableProcessors</name>
                    <dataType>CoresAvailable</dataType>
                    <updateType>Conditional</updateType>
                    <updateCondition>On change</updateCondition>
                    <ownership>NoTransfer</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <dimensions/>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>Available Core Processors.</semantics>
                </attribute>
                <attribute>
                    <name>TotalMemory</name>
                    <dataType>TotalMemory</dataType>
                    <updateType>Conditional</updateType>
                    <updateCondition>On change</updateCondition>
                    <ownership>NoTransfer</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <dimensions/>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>Total memory.</semantics>
                </attribute>
                <attribute>
                    <name>FreeMemory</name>
                    <dataType>FreeMemory</dataType>
                    <updateType>Conditional</updateType>
                    <updateCondition>On change</updateCondition>
                    <ownership>NoTransfer</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <dimensions/>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>Free memory.</semantics>
                </attribute>
            </objectClass>
			
            <objectClass>
                <name>Core</name>
                <sharing>PublishSubscribe</sharing>
                <semantics>A Computer Processor Core Representation</semantics>
                <attribute>
                    <name>OwnerNode</name>
                    <dataType>HLAunicodeString</dataType>
                    <updateType>Static</updateType>
                    <updateCondition>NA</updateCondition>
                    <ownership>NoTransfer</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <dimensions/>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>Owner Computer's MAC Address</semantics>
                </attribute>
				
                <attribute>
                    <name>SerialNumber</name>
                    <dataType>HLAunicodeString</dataType>
                    <updateType>Static</updateType>
                    <updateCondition>NA</updateCondition>
                    <ownership>NoTransfer</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <dimensions/>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>Serial number of the node</semantics>
                </attribute>
				
                <attribute>
                    <name>IsWorking</name>
                    <dataType>HLAboolean</dataType>
                    <updateType>Static</updateType>
                    <updateCondition>NA</updateCondition>
                    <ownership>NoTransfer</ownership>
                    <sharing>PublishSubscribe</sharing>
                    <dimensions/>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                    <semantics>Serial number of the node</semantics>
                </attribute>
				
            </objectClass>
			
        </objectClass>
    </objects>
	
    <interactions>
        <interactionClass>
            <name>HLAinteractionRoot</name>
            <interactionClass>
                <name>RunActivity</name>
                <sharing>PublishSubscribe</sharing>
                <dimensions/>
                <transportation>HLAreliable</transportation>
                <order>Receive</order>
                <semantics>Command a Core to Run an Activity</semantics>
                <parameter>
                    <name>ActivityContent</name>
                    <dataType>HLAunicodeString</dataType>
                    <semantics>The Hex String representing the compressed Instance</semantics>
                </parameter>
                <parameter>
                    <name>SerialNumber</name>
                    <dataType>HLAunicodeString</dataType>
                    <semantics>The serial number of core to run this activity</semantics>
                </parameter>
            </interactionClass>

        </interactionClass>
    </interactions>
	
    <switches>
        <autoProvide isEnabled="true"/>
        <conveyRegionDesignatorSets isEnabled="false"/>
        <conveyProducingFederate isEnabled="false"/>
        <attributeScopeAdvisory isEnabled="false"/>
        <attributeRelevanceAdvisory isEnabled="false"/>
        <objectClassRelevanceAdvisory isEnabled="false"/>
        <interactionRelevanceAdvisory isEnabled="false"/>
        <serviceReporting isEnabled="false"/>
        <exceptionReporting isEnabled="false"/>
        <delaySubscriptionEvaluation isEnabled="false"/>
        <automaticResignAction resignAction="CancelThenDeleteThenDivest"/>
    </switches>
	
    <dataTypes>
        <simpleDataTypes>
		
            <simpleData>
                <name>FreeMemory</name>
                <representation>HLAinteger32BE</representation>
                <units>Bytes</units>
                <resolution>1</resolution>
                <accuracy>1</accuracy>
                <semantics>Integer to describe free RAM memory.</semantics>
            </simpleData>

            <simpleData>
                <name>TotalMemory</name>
                <representation>HLAinteger32BE</representation>
                <units>Bytes</units>
                <resolution>1</resolution>
                <accuracy>1</accuracy>
                <semantics>Integer to describe total RAM memory.</semantics>
            </simpleData>
			
            <simpleData>
                <name>CoresAvailable</name>
                <representation>HLAinteger32BE</representation>
                <units>Units</units>
                <resolution>1</resolution>
                <accuracy>1</accuracy>
                <semantics>Integer to describe cores available.</semantics>
            </simpleData>

            <simpleData>
                <name>CpuFloat</name>
                <representation>HLAfloat32BE</representation>
                <units>Percents</units>
                <resolution>0.001</resolution>
                <accuracy>0.001</accuracy>
                <semantics>CPU Load.</semantics>
            </simpleData>
        </simpleDataTypes>
		
        <enumeratedDataTypes/>
        <arrayDataTypes/>
        <fixedRecordDataTypes/>
        <variantRecordDataTypes/>
    </dataTypes>

</objectModel>
